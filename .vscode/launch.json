{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/build/u-boot/u-boot",
            "args": [

            ],
            "externalConsole": true,
            "stopAtEntry": true,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "miDebuggerPath": "/opt/aarch64-none-linux-gnu/bin/aarch64-none-linux-gnu-gdb",
            "MIMode": "gdb",
            "miDebuggerServerAddress": "localhost:9500",
            "showDisplayString": true,
            "targetArchitecture": "arm64",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Enable display hex value as default",
                    "text": "set output-radix 16"
                }
            ],
            "logging": {
                "engineLogging": false,
                "moduleLoad": true,
                "exceptions": true,
                "trace": false,
                "traceResponse": false,
                "programOutput": true
            }
        }
    ]
}